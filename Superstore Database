CREATE TABLE superstore (
    item_id INTEGER PRIMARY KEY,
    item_name TEXT,
    category TEXT,
    price DECIMAL(10, 2),
    stock_quantity INTEGER,
    average_rating DECIMAL(3, 2)
);

SELECT *
FROM superstore

SELECT *
FROM superstore
WHERE price>0
ORDER BY price DESC

SELECT AVG(price)
FROM superstore
WHERE price>0

SELECT 'Kitchen Supplies'
FROM superstore
WHERE average_rating>4.5

SELECT *
FROM customer_subscriptions
WHERE product_id=1;

SELECT email
FROM customer_subscriptions
WHERE product_id
IN (SELECT product_id
    FROM products
    WHERE product_type="Internet");



